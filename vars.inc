;
; переменные для работы с консолью
;
stdout    DD ?	
stdin     DD ?
cWritten  DD ?
cEmpty    DB ?
EOL       DB 13,10
SPACE     DB ' '
COMMA     DB ','
_rSeed    DD 12345678h

;
; строковая переменная для отображения числа, для 32-битных чисел - максимум 10 символов
;
buf     DB 11 DUP(0)

;
; временная переменная для вывода непосредственно заданной строки на экран
;
_strbuf DB 1024 DUP(0)

